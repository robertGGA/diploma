        "dev": "nodemon -e ts,js --exec ts-node -r tsconfig-paths/register index.ts",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
        "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
        "migration:create": "npm run typeorm migration:create",
        "migration:generate": "npm run typeorm migration:generate -- -d ./src/db/db.ts",
        "migration:show": "npm run typeorm migration:show -- -d ./src/db/db.ts",
        "schema:sync": "npm run typeorm schema:sync -- -d ./src/db/db.ts",
        "docker:run": "docker compose up -d",
        "lint": "npx eslint --ext .ts ./ --fix"
